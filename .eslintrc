{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "no-console": "off",
        "react/jsx-uses-vars": 2,
    },
    "extends": "eslint:recommended",
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}